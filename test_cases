%yes
total_people([[0],[0]],C). C = 0 # Sin habitantes
total_people([[0,0,s(0)],[0,0]],C). C = 0 # No existe la columna
total_people([[0,0],[0,0]],C). C = 0 # Sin habitantes
total_people([[0,0,0],[0,0,0],[0,0,0]],C). C = 0 # Sin habitantes
total_people([[s(0),s(s(s(0)))],[0,s(s(0))]],C). C = s(s(s(s(s(s(0))))))
