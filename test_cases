%yes
columns([[0],[0]],C). C = [[0,0]] # Sin habitantes
columns([[0,0,s(0)],[0,0]],C). C = [[0,0],[0,0],[s(0)]] # No existe la columna
columns([[0,0],[0,0]],C). C = [[0,0],[0,0]] # Sin habitantes
columns([[0,0,0],[0,0,0],[0,0,0]],C). C = [[0,0,0],[0,0,0],[0,0,0]] # Sin habitantes
columns([[s(0),s(s(0)),0],[0,s(s(s(0))),0],[s(0),s(0),s(0)]],C). C = [[s(0),0,s(0)],[s(s(0)),s(s(s(0))),s(0)],[0,0,s(0)]]# Varios habitantes por nivel
